{
  "name": "json2ppt-editor",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build:json2pptx": "pnpm -C src/lib/json2pptx build",
    "build:pptx-previewer": "pnpm -C src/lib/pptx-previewer build",
    "build:pptx-custom": "pnpm -C src/lib/pptx-custom build",
    "dev": "vite",
    "predev": "pnpm -C src/lib/json2pptx build && pnpm -C src/lib/pptx-previewer build && pnpm -C src/lib/pptx-custom build",
    "prebuild": "pnpm -C src/lib/json2pptx install --frozen-lockfile && pnpm -C src/lib/json2pptx build && pnpm -C src/lib/pptx-previewer install --frozen-lockfile && pnpm -C src/lib/pptx-previewer build && pnpm -C src/lib/pptx-custom install --frozen-lockfile && pnpm -C src/lib/pptx-custom build",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-select": "^2.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "tinycolor2": "^1.6.0",
    "lucide-react": "^0.563.0",
    "monaco-editor": "^0.52.0",
    "json2pptx": "workspace:*",
    "pptx-custom": "workspace:*",
    "pptx-previewer": "workspace:*",
    "pptxtojson": "^1.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "txml": "^5.1.1",
    "buffer": "^6.0.3",
    "process": "^0.11.10"
  },
  "devDependencies": {
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.8"
  }
}
