{
  "name": "pptx-custom",
  "version": "0.1.2",
  "description": "Custom content and theme utilities for PPTX JSON templates.",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://json2pptx.henryge.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/gezhiheng/json2ppt-editor/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.6.3"
  }
}
