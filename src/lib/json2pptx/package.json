{
  "name": "json2pptx",
  "version": "0.4.1",
  "description": "Convert JSON slide definitions to PPTX using PptxGenJS.",
  "homepage": "https://json2pptx.henryge.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/gezhiheng/json2ppt-editor/"
  },
  "license": "MIT",
  "author": "",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev:watch": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "json2pptx-schema": "0.1.1",
    "jszip": "^3.10.1",
    "pptxgenjs": "^3.12.0",
    "svg-arc-to-cubic-bezier": "^3.2.0",
    "svg-pathdata": "^6.0.1",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/tinycolor2": "^1.4.6",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}